# GitAlong Environment Configuration
# Copy this file to .env and fill in your actual values

# App Configuration
APP_NAME=GitAlong
ENVIRONMENT=production
ENABLE_ANALYTICS=true
ENABLE_DEBUG_LOGGING=false
API_TIMEOUT_SECONDS=30

# GitHub OAuth Configuration (Required for GitHub integration)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_REDIRECT_URI=com.gitalong.app://oauth/callback

# Firebase Configuration (Auto-generated by flutterfire configure)
# These are automatically handled by firebase_options.dart

# Backend API Configuration
BACKEND_API_URL=https://api.gitalong.com
BACKEND_API_KEY=your_backend_api_key_here

# Email Service Configuration (Optional)
EMAIL_SERVICE_API_KEY=your_email_service_key_here
EMAIL_FROM_ADDRESS=noreply@gitalong.com

# Analytics Configuration (Optional)
MIXPANEL_TOKEN=your_mixpanel_token_here
SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
ENABLE_ML_MATCHING=true
ENABLE_PUSH_NOTIFICATIONS=true
ENABLE_EMAIL_NOTIFICATIONS=true
ENABLE_SOCIAL_FEATURES=true

# Security Configuration
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Database Configuration (For backend)
DATABASE_URL=your_database_url_here
REDIS_URL=your_redis_url_here

# Third-party Services
STRIPE_PUBLISHABLE_KEY=your_stripe_key_here
STRIPE_SECRET_KEY=your_stripe_secret_here

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_PERFORMANCE_MONITORING=true
ENABLE_ERROR_TRACKING=true 